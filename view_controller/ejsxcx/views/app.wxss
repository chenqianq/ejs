/**app.wxss**/
page {
	width: 100%; height: 100%;
	font-size: 26rpx;
	color: #333;
}
.flex {
	display: flex;
	display: -webkit-flex;
}
.H10 {
	height: 20rpx; width: 100%;
	background: #fafafa;
}
.end-tip{
	height: 100rpx; line-height: 100rpx;
	text-align: center;
	color: #aaa;
	font-size: 24rpx;
}
.one-line {
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}
button::after {
  border: 0;
}
