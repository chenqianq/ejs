
.cart {
	height: 100%; width: 100%;
}

/* 为空 */
.cart .empty {
	flex-direction: column;
  width: 100%;
	height: 100%;
	align-items: center;
}
.cart .empty image {
	width: 120rpx; height: 120rpx;
	margin-top: 330rpx;
	margin-bottom: 40rpx;
}

/* 不为空 */
.cart .cart-wrap .tit {
	height: 64rpx; line-height: 64rpx;
	padding-left: 50rpx;
	color: #999496;
  border-top: 1px solid #eee;
}
.cart .cart-wrap .tit .t2 {
	color: #ff658f;
}
.cart .list-wrap {
	width: 690rpx;
	margin: 0 auto 140rpx;
	padding: 20rpx 0;
	border: 2rpx solid #eee;
	border-radius: 12rpx;
}
.cart .list-wrap .item {
	width: 100%; height: 220rpx;
}
.isEditCheckBox {
  position: relative;
  z-index: 998;
}
.cart .list-wrap .item .check {
  position: relative;
  z-index: 998;
	width: 40rpx; height: 40rpx;
	margin: 80rpx 0 0 20rpx;
}
.cart .list-wrap .item .hide {
	display: none;
}

.cart .list-wrap .cover {
	width: 200rpx; height: 200rpx;
}
.cart .list-wrap .info {
	position: relative;
	width: 480rpx;
}
.cart .list-wrap .info .title {
	font-size: 30rpx;
}
.cart .list-wrap .info .size {
	margin-top: 12rpx;
	color: #999;
	font-size: 26rpx;
}

.cart .list-wrap .info .price-wrap {
	position: absolute;
	bottom: 32rpx; left: 0;
	width: 430rpx;
}

.cart .list-wrap .price {
	flex: 1;
	color: #ff7198;
}
.cart .list-wrap .price text {
	font-size: 36rpx;
}
.cart .list-wrap .num-wrap {
	margin-right: 20rpx;
}
.cart .list-wrap .price {
	color: #ff658f;
	font-weight: bold;
}
.cart .list-wrap .add,
.cart .list-wrap .reduce {
	display: inline-block;
	width: 50rpx; height: 50rpx;
	line-height: 48rpx;
	border: 2rpx solid #999;
  color: #999;
	border-radius: 50rpx;
	text-align: center;
}
.cart .list-wrap .val {
	display: inline-block;
	width: 50rpx; line-height: 50rpx;
	text-align: center;
}

.cart .fixbar {
	position: fixed;
	bottom: 0;
	width: 100%; height: 100rpx;
	line-height: 100rpx;
	border-top: 2rpx solid #eee;
	background: #fff;
  z-index: 9999;
}
.cart .fixbar .all {
	width: 180rpx;
	padding-left: 44rpx;
}
.cart .fixbar .total {
	width: 320rpx;
	margin-right: 20rpx;
	flex-direction: column;
	text-align: right;
	line-height: 50rpx;
}
.cart .fixbar .total view {
	color: #333;
	font-weight: bold;
}
.cart .fixbar .total .t1 {
	color: #ff658f;
  font-size: 36rpx;
  padding-left: 6rpx;
}
.cart .fixbar .total .t2 {
	color: #999;
	font-size: 24rpx;
  line-height: 36rpx;
}
.cart .fixbar .navigatorSub {
	width: 260rpx; height: 80rpx;
	line-height: 80rpx;
	margin: 10rpx;
	background: #f63a65;
	color: #fff;
	border-radius: 10rpx;
	text-align: center;
	font-size: 28rpx;
}
.cart .fixbar .btn-wrap {
	width: 520rpx;
	text-align: right;
	flex-direction: row-reverse;
}
.cart .fixbar .btn-wrap button {
	width: 260rpx; height: 80rpx;
	line-height: 80rpx;
	margin: 10rpx;
	background: #d3d3d3;
	color: #fff;
	border-radius: 10rpx;
	text-align: center;
	font-size: 28rpx;
}
.H90 {
  width: 100%;
  height: 90rpx;
}
.cart .list-wrap .cant_add {
  border-color: #eee;
  color: #eee;
}
.cart .list-wrap .price .list_price {
  margin-left: 16rpx;
  color: #bbb;
  font-size: 22rpx;
  font-weight: normal;
  text-decoration: line-through;
}
.visCheckAll {
  position: absolute;
  top: 28rpx;
  left: 40rpx;
  background-color: #ffffff;
  border: 1px solid #d4d4d9;
  border-radius: 50%;
  width: 40rpx;
  height: 40rpx;
  z-index: 1;
}
.visCheckAll.isClick {
  background-color: #f63a65;
  border-color: #f63a65;
}
.all checkbox {
  position: relative;
  z-index: 2;
  opacity: 0;
}
.visCheckAll image {
  position: absolute;
  top: 0;
  left: 0;
  width: 40rpx;
  height: 40rpx;
}
.tWrap {
  line-height: 36rpx;
  margin-top: 14rpx;
}